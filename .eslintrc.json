{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:react-hooks/recommended"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint", "react-hooks", "import"],
  "rules": {
    "accessor-pairs": 2,
    "array-bracket-spacing": 2,
    "array-callback-return": 2,
    "arrow-spacing": 2,
    "block-scoped-var": 2,
    "block-spacing": 2,
    "brace-style": 0,
    "callback-return": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "complexity": 0,
    "computed-property-spacing": 2,
    "consistent-return": 0,
    "consistent-this": 0,
    "curly": [2, "multi-line"],
    "default-case": 2,
    "dot-location": 0,
    "dot-notation": 2,
    "eol-last": "off",
    "eqeqeq": 2,
    "func-names": 2,
    "func-style": 0,
    "generator-star-spacing": 2,
    "global-require": 0,
    "guard-for-in": 0,
    "handle-callback-err": 2,
    "id-blacklist": 2,
    "id-length": 0,
    "id-match": 2,
    "import/default": 2,
    "import/export": 2,
    "import/namespace": 2,
    "indent": 0,
    "init-declarations": 2,
    "key-spacing": 2,
    "keyword-spacing": 2,
    "lines-around-comment": 0,
    "max-depth": 2,
    "max-nested-callbacks": 2,
    "max-params": 1,
    "max-statements": 0,
    "max-statements-per-line": 2,
    "multiline-ternary": 0,
    "new-cap": 2,
    "newline-after-var": 0,
    "newline-before-return": 2,
    "newline-per-chained-call": 0,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-confusing-arrow": [
      "error",
      {
        "allowParens": false
      }
    ],
    "no-console": 0,
    "no-continue": 0,
    "no-div-regex": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 2,
    "no-empty-function": 0,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-extra-parens": 0,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 0,
    "no-lonely-if": 0,
    "no-loop-func": 0,
    "no-magic-numbers": 0,
    "no-mixed-requires": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 0,
    "no-multiple-empty-lines": 2,
    "no-negated-condition": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 0,
    "no-path-concat": 2,
    "no-process-env": 0,
    "no-process-exit": 2,
    "no-proto": 2,
    "no-prototype-builtins": 0,
    "no-restricted-globals": 2,
    "no-restricted-imports": [
      "error",
      {
        "message": "Please use lodash-es instead.",
        "name": "lodash"
      }
    ],
    "no-restricted-modules": 2,
    "no-restricted-syntax": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sync": 2,
    "no-ternary": 0,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef-init": 2,
    "no-undefined": 0,
    "no-unmodified-loop-condition": 2,
    "no-unneeded-ternary": 2,
    "no-unused-expressions": 0,
    "no-use-before-define": 2,
    "no-useless-call": 2,
    "no-useless-computed-key": 2,
    "no-useless-concat": 2,
    "no-useless-constructor": 2,
    "no-useless-escape": 2,
    "no-useless-rename": 2,
    "no-var": 0,
    "no-void": 2,
    "no-warning-comments": 2,
    "no-whitespace-before-property": 2,
    "no-with": 2,
    "object-curly-newline": 0,
    "object-property-newline": 0,
    "object-shorthand": 2,
    "one-var": 0,
    "one-var-declaration-per-line": 2,
    "operator-assignment": 2,
    "operator-linebreak": 0,
    "padded-blocks": 0,
    "prefer-const": 0,
    "prefer-reflect": 0,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 1,
    "quote-props": 0,
    "radix": 2,
    "react-hooks/exhaustive-deps": 1,
    "react-hooks/rules-of-hooks": 1,
    "react/display-name": 1,
    "react/jsx-boolean-value": 2,
    "react/jsx-uses-react": "off",
    "react/prop-types": 1,
    "react/react-in-jsx-scope": "off",
    "require-jsdoc": 0,
    "rest-spread-spacing": 2,
    "semi": 2,
    "semi-spacing": 2,
    "sort-imports": 0,
    "sort-vars": 0,
    "space-before-blocks": 2,
    "space-before-function-paren": 0,
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2,
    "strict": 2,
    "template-curly-spacing": 2,
    "unicode-bom": ["error", "never"],
    "vars-on-top": 2,
    "wrap-iife": 2,
    "yield-star-spacing": 2,
    "yoda": 2
  },

  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".ts", ".tsx"],
        "moduleDirectory": ["node_modules", "src/"]
      }
    }
  }
}
